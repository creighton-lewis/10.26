<!--
  Example Nmap XML (Nmap 7.90, generated for lab use)
  Contains a vulners script result in both JSON and plain‑text format.
-->
<nmaprun scanner="nmap" args="nmap -sS -p 21,80 -A 127.0.0.1" start="1700000000" startstr="Thu Apr 27 12:00:00 2024" version="7.90" xmloutputversion="1.04">
  <scaninfo type="syn" protocol="tcp" numservices="2" services="21,80"/>
  <host>
    <status state="up" reason="hostup" reason_ttl="0"/>
    <address addr="127.0.0.1" addrtype="ipv4"/>
    <hostnames>
      <hostname name="localhost" type="user" method="default"/>
    </hostnames>
    <ports>
      <!-- Open FTP port (high‑risk) -->
      <port protocol="tcp" portid="21">
        <state state="open" reason="syn-ack" reason_ttl="0"/>
        <service name="ftp" product="ProFTPD 1.3.5c" version="1.3.5c" extrainfo="vs FTP server" method="table" conf="3"/>
      </port>
      <!-- Open HTTP port (low risk) -->
      <port protocol="tcp" portid="80">
        <state state="open" reason="syn-ack" reason_ttl="0"/>
        <service name="http" product="Apache httpd" version="2.4.48" extrainfo="Apache/2.4.48 (Ubuntu) PHP/8.1.10" method="table" conf="3"/>
      </port>
    </ports>
    <!-- 1.  JSON‑style vulners output (Nmap ≥7.60) -->
    <script id="vulners" output='{"vulners":[{"cve":"CVE-2023-1234","description":"Example FTP vulnerability"},{"cve":"CVE-2021-5678","description":"Some other CVE"}]}'/>
    <!-- 2.  Fallback plain‑text vulners script result -->
    <script id="vulners" output="
CVE-2020-9999: Example legacy vulnerability on port 21
CVE-2019-8888: Obsolete HTTP service flaw
"/>
  </host>
</nmaprun>
